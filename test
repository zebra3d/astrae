   jump roue

label roue1:
   $ gold -= 10
   "Choisissez une couleur"
   $ couleur = 0
   $ couleur += renpy.random.randint(1,6)
   menu roue2:
       "Bleu":
           $ bleu = 1
           jump roueresultat

       "Rouge":
           $ rouge = 1
           jump roueresultat

       "Vert":
           $ vert = 1
           jump roueresultat

       "Jaune":
           $ jaune = 1
           jump roueresultat

       "Noir":
           $ noir = 1
           jump roueresultat

       "Blanc":
           $ blanc = 1
           jump roueresultat
   jump roue2

