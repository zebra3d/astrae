label rochage1:
   if casque == 1:
       "Heureusement, vous aviez un casque, tout va bien."
       jump rocaille
   if imprudent == 1:
       "Les gravats tombent sur vous, la blessure est plus dure que prévue…"
       "Vous perdez 10 pv."
       $ mental -= 5
       if mental <= 0:
           jump crise
       $ vie -= 10
       if vie <= 0:
           jump mort
       jump rocaille
   if agile == 1:
       "Grâce à votre agilité, vous parvenez à esquiver les pierres."
       $ mental += 5
       if mental >= 100:
           $ mental = 100
       jump rochage
   if tenace == 1:
       "Grâce à votre ténacité, les gravats ne vous font rien."
       $ mental += 2
       if mental >= 100:
           $ mental = 100
       if mental <= 0:
           jump crise
       $ vie += 5
       if vie <= 0:
           jump mort
       jump rocaille
   "A cause d'une faute d'inattention, vous vous faites très mal."
   $ vie -= 10
   if vie <= 0:
       jump mort
   $ mental -= 20
   if mental <= 0:
       jump crise
   jump rocaille

