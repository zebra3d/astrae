label obscuros1:
   if immature == 1:
       "La lumiere reste constante face à vous."
       $ mental -= 20
       if mental <= 2:
           jump crise
       jump obscur3
   if timide == 1:
       "La lumiere reste constante face à vous."
       $ mental -= 20
       if mental <= 2:
           jump crise
       jump obscur3
   if amical == 1:
       "Les ténèbres que vous engendrez deviennent plus intenses."
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump obscur3
   if fiable == 1:
       "Les ténébre que vous engendrez deviennent plus intenses."
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump obscur3
   "La lumiere reste constante face à vous."
   $ mental -= 40
   if mental <= 2:
       jump crise
   jump obscur3

