label vieux:
   if logique == 1:
       "Grâce à votre sens de la logique, vous parvenez à aider le vieil homme."
       "Vieille Homme" "Donnez ce portrait à mon frère."
       "Tu remarques que sa respiration diminue…"
       "Qu'il finit par s'éteindre…"
       "Vous décidez de laisser son corps ici et de repartir voir les autres."
       $ portrait = 1
       $ inventaire.append(portrait2)
       $ mental += 15
       if mental >= 100:
           $ mental = 100
       jump cale
   if persuasif == 1:
       "Vous écoutez le vieil homme..."
       "Vieille Homme" "Donnez ce portrait à mon frère."
       "Tu remarques que sa respiration diminue."
       "Qu'il finit par s'éteindre…"
       "Vous décidez de laisser son corps ici et de repartir voir les autres."
       $ mental += 5
       $ portrait = 1
       $ inventaire.append(portrait2)
       if mental >= 100:
           $ mental = 100
       jump cale
   if efficace == 1:
       "Vous écoutez le vieil homme..."
       "Vieille Homme" "Donnez ce portrait à mon frère."
       "Tu remarques que sa respiration diminue…"
       "Qu'il finit par s'éteindre…"
       "Vous décidez de laisser son corps ici et de repartir voir les autres."
       $ mental += 5
       $ portrait = 1
       $ inventaire.append(portrait2)
       if mental >= 100:
           $ mental = 100
       jump cale
   if prudent == 1:
       "Vous écoutez le vieil homme..."
       "Vieille Homme" "Donnez ce portrait à mon frère."
       "Tu remarques que sa respiration diminue…"
       "Qu'il finit par s'éteindre…"
       "Vous décidez de laisser son corps ici et de repartir voir les autres."
       $ mental += 2
       $ portrait += 1
       $ inventaire.append(portrait2)
       if mental >= 100:
           $ mental = 100
       jump cale
   "Vous n'avez aucune capacité requise pour aider le vieil homme, il succombe soudainement…"
   "Vous perdez -20 mental."
   $ mental -= 20
   if mental <= 0:
       jump crise
   jump cale

