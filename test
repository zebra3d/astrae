label victoirearene:
   "Bunda" "Vous avez réussi à me vaincre."
   "Bunda" "Voulez vous recommencez les arene?{a=jump:arene}Oui{/a}"
   "Bon, vous pouvez saluer la foule…"
   "{a=jump:arenec}Vulgaire{/a} ; {a=jump:arenec}Moche{/a} ; {a=jump:arenec}Timide{/a} ; {a=jump:arenec}Arrogant{/a} ; {a=jump:arenec}instable{/a} ; {a=jump:arenec1}Optimiste{/a} ; {a=jump:arenec1}Charmeur{/a} ; {a=jump:arenec1}Mysterieux{/a} ; {a=jump:arenec1}Drole{/a} ; {a=jump:arenec1}Magnifique{/a} ; {a=jump:arenec1}AmisDesBulles{/a} ; {a=jump:arenec2}Charismatique{/a} ; {a=jump:foulec}Rien de tous ça{/a}"
   jump victoirearene

label arenec:
   if vulgaire == 1:
       "Vous faites un bras d'honneur à la foule…"
       "En leur disant {b}JE VOUS EMMERDE!{/b}"
       $ mental -= 20
       jump foulec
   if moche == 1:
       "Vous êtes tellement laid que la foule ne vous aime pas trop…"
       $ mental -= 5
       jump foulec
   if timide == 1:
       "Vous êtes trop timide pour prendre la parole…"
       $ mental -= 5
       jump foulec
   if arrogant == 1:
       "Vous faites le crâneur devant la foule…"
       $ mental -= 20
       jump foulec
   if instable == 1:
       "Une personnalité prend le dessus sur vous… mais qui?"
       $ personalite = 0
       $ personalite += renpy.random.randint(1,5)
       if personalite == 1:
           "Louis ou Tom" "Je suis la personne qui a vaincu le monde."
           "Louis ou Tom" "Je suis un super guerrier, vous savez."
           "Vous perdez  -10 mental & +5 vie."
           $ mental -= 10
           $ vie += 5
           jump foulec
       if personalite == 2:
           "KevinLeThug" "UESH LES GENS BIEN OU BIEN?"
           $ mental -= 10
           jump foulec
       if personalite == 3:
           "Alexa" "Je suis votre robot à votre service."
           $ mental += 5
           jump foulev
       if personalite == 4:
           "Timmy a peur de la foule."
           $ mental -= 5
           jump foulec
       if personalite == 5:
           "Faux Narrateur" "Vous avez l'immense privilège d'avoir vu le grand narrateur !"
           $ mental -= 20
           jump foulec
   "Vous ne savez pas trop quoi dire…"
   "La pression de l'arène fait que vous perdez -20 mental."
   $ mental -= 20
   jump foulec

label arenec1:
   if optimiste == 1:
       "Tu salut la foule d'un air humble."
       "Tu gagnes + 10 mental."
       $ mental += 10
       jump foulev
   if charmeur == 1:
       "Tu remercies la foule en faisant le symbole du cœur avec vos mains."
       "Tu gagnes +10 mental."
       $ mental += 10
       jump foulev
   if drole == 1:
       "Tu fais le pitre devant la foule pour les faire rigoler…"
       "T’as plutôt du succès…"
       $ mental += 15
       jump foulev
   if mysterieux == 1:
       "Ton côté mystérieux fait sensation auprès de la foule!"
       $ mental += 20
       jump foulev
   if magnifique == 1:
       "Ta beauté fait craquer la foule!"
       $ mental += 10
       jump foulev
   if bulle == 1:
       h "Blougloublou"
       "La foule te regarde avec un air bizarre… mais ton côté bizarre fait apprécier la foule."
       $ mental += 20
       jump foulev
   "Vous ne savez pas trop quoi dire…"
   "La pression de l'arène fait que vous perdez -20 mental."
   $ mental -= 20
   jump foulec

