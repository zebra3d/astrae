label narrateurvrai:
   hide screen narrateur1
   hide screen narrateur2
   hide screen narrateur3
   hide screen narrateur4
   hide screen narrateur5
   show narrateur
   "Narrateur" "Bravo joueur! effectivement, cette image de moi me met très en valeur!"
   "Narrateur" "Tu as du goût pour un joueur! bon, maintenant que tu es dans mon monde, tu peux faire ce que bon te semble."
   "Le narrateur te rend la vie plus facile et te rend toute ta vie."
   $ vie = 100
   hide narrateur
   "{a=jump:lieux1}retournez à l'Aventure{/a};{a=jump:click}Cliquer Ici{/a};{a=jump:ncombat}Attaquer le Narrateur{/a}"
   jump narrateurvrai

label click:
   show peur
   play sound scary
   "BOUH"
   hide peur
   show narrateur
   "Narrateur" "Mwahahaha t’as vraiment cru que un trouduq comme toi allez pouvoir faire ce qu'il veut et cliquer n'importe ou !?"
   "Narrateur" "Désolé joueur, mais je ne te laisserais pas faire !"
   hide narrateur
   jump ncombat

label ncombat:
   show narrateur
   with vpunch
   if vie <= 0:
       hide narrateur
       jump mort
   if creature >= 1:
       show narrateuraie
       "Narrateur" "Et merde… j'ai mis 1d100 d'attaque mais j'ai pas pris de point de vie.."
       hide narrateuraie
       jump narrateurmalefique
   play music darkfight
   "Le Narrateur se dresse devant vous."
   "Le combat s'engage."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,100)
   "Le Narrateur vous porte 1d100 dégât."
   "Il vous reste [vie] vie."
   hide narrateur
   "{a=jump:ncombat}frapper encore{/a} ; "
   jump ncombat

label narrateurmalefique:
   show narrateur
   "Narrateur" "Le Combat n'est pas encore terminé…"
   hide narrateur
   "Le narrateur se transforme."
   show narrateurtenebre1
   with vpunch
   "Narrateur Maléfique" "Maintenant on va jouer!"
   "Le Narrateur vous propose un concours de blague."
   "Il commence, choisissez sa pire blague."
   hide narrateurtenebre1
   "{a=jump:blague}La Mère du Joueur est tellement grosse qu’elle passe pas la porte{/a};{a=jump:rip}Cliquer Ici{/a};{a=jump:pireblague}Les pigeons de timmy sont tellement con qu'ils raquettes un gosse{/a}"
   jump narrateurmalefique

label blague:
   "Cette blague vous a touché, vous perdez -20 mental."
   $ mental -= 20
   if mental <= 0:
       jump crise
   jump narrateurmalefique

label rip:
   show peur
   play sound scary
   "Aie, cette blague vous a grandement touchée ! vous perdez -60 mental."
   hide peur
   $ mental -= 60
   if mental <= 0:
       jump crise
   jump narrateurmalefique

label pireblague:
   "Bravo, vous avez touché le narrateur maléfique, vous avez trouvé sa pire blague !"
   "Vous gagnez + 20 mental."
   $ mental += 20
   jump narrateurmalefique2

label narrateurmalefique2:
