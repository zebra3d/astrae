label cricri:
   $ energie = 100
   show punika
   "Punika" "C’est bon tu t'es bien équipé ?"
   "Punika" "Parfait !"
   "Punika" "Maintenant il est temps pour toi d'apprendre quelques techniques de combat."
   "Punika" "Pour la bataille qui s'engage, il est important de s'entraîner"
   $ coeur = 0
   $ rugissement = 0
   $ colere = 0
   menu krops:
       "Apprendre coeur vaillant":
           "Vous rend des points de vie à chaque tour de combat"
           "Vous coute 20 energie"
           "Et tu sur de vouloir apprendre cette technique?"
           "{a=jump:coeurvaillant01}Oui{/a} ;{a=jump:krops}Non{/a} ;"
           jump krops

       "Apprendre rugissement":
           "Vous ajoute 20 attaque à chaque tour"
           "Vous coute 20 energie"
           "Et tu sur de vouloir apprendre cette technique?"
           "{a=jump:rugissement01}Oui{/a} ;{a=jump:krops}Non{/a} ;"
           jump krops

       "Apprendre colère bestiale":
           "Faite une attaque très puissante"
           "Vous coute 50 energie"
           "Et tu sur de vouloir apprendre cette technique?"
           "{a=jump:colere01}Oui{/a} ;{a=jump:krops}Non{/a} ;"
           jump krops

