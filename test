label crevassev2:
   if maudit == 1:
       "Vous ne savez pas pourquoi,"
       "Mais vous vous sentez très très mal!"
       $ mental -= 20
   if poupe == 1:
       "Vous ne savez pas pourquoi,"
       "Mais vous ne vous sentez pas bien…"
       $ mental -= 10
   scene black
   stop music
   play music alchemist
   "[carte]"
   "Vous retrouvez Yae sur votre chemin."
   "Vous cherchez un nouvel endroit où aller."
   if carte == 8:
       jump finnarrateur1
   "Sur votre route, vous rencontrez un marchand."
   $ pet = 0
   jump familier

