label robot1:
   if instable == 1:
       $ personalite = renpy.random.randint(1,5)
       if personalite == 1:
           "Louis ou Tom" "Je vais le réparer, alors… le fil bleu dans le fil rouge…"
           $ vie += 5
           $ mental -= 10
           if mental <= 0:
               jump crise
           jump robot
       if personalite == 2:
           "Kevin Le Thug" "Uesh gros azy relève toi fait pas ton crevard."
           $ mental -= 10
           if mental <= 0:
               jump crise
           jump robot
       if personalite == 3:
           "Alexa" "Vous êtes mon ami robot, je vais vous réparer en moins de deux."
           "Vous réussissez à réparer le robot."
           $ mental += 5
           if mental >= 100:
               $ mental = 100
           jump robot
       if personalite == 4:
           "Timmy" "Oskour! un robot démoniaque!!!"
           "Vous vous faites pipi dessus…"
           $ mental -= 5
           if mental <= 0:
               jump crise
           jump robot
       if personalite == 5:
           "Faux Narrateur" "D'après mon manuel du narrateur, il faut enrouler ce fils autour du cou…"
           $ mental -= 20
           if mental <= 0:
               jump crise
           jump robot
   if paresseux == 1:
       "A cause de votre paresse, vous avez du mal à vous mettre à l'action…"
       $ mental -= 5
       if mental <= 0:
           jump crise
       $ attaque -= 2
       jump robot
   if efficace == 1:
       "Grâce à votre efficacité, vous réparez le robot."
       $ mental += 5
       if mental >= 100:
           $ mental = 100
       jump robot
   if logique == 1:
       "Grâce à votre logique, vous réparez le robot."
       $ mental += 15
       if mental >= 100:
           $ mental = 100
       jump robot
   if farfouilleur == 1:
       "Grâce à vos talents de farfouilleur, vous branchez le fil rouge avec le file vert."
       $ mental += 2
       if mental >= 100:
           $ mental = 100
       jump robot
   "Vous ne trouvez rien à faire pour le réparer..."
   "Vous perdez -20 mental."
   $ mental -= 20
   if mental <= 0:
       jump crise
   jump robot

