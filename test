label cricri1:
   scene cristaline
   show punika
   play music astre
   "Punika" "Nous y voici !"
   "Punika" "Voilà l'ange de la vie"
   "Punika" "Salutation Oh grand ange"
   "Punika" "Nous sommes venu vous prévenir d'une attaque imminente !"
   hide punika
   show akame
   "Akame" "Je le sais, fort bien malheureusement… nos moyens de défense sont très réduits…"
   "Akame" "Si vous pouviez nous aider j'en serais ravi…"
   "Akame" "Comment vous appelez vous?"
   hide akame
   show punika
   "Punika" "Je suis Punika chef des Virvil."
   "Punika" "Et voici [player_name]."
   hide punika
   show akame
   "Akame" "Punika… j'ai entendu parler de votre histoire. Si vous m'aidez je serais en mesure de vous rendre la pareille."
   "Akame" "Quant à toi [player_name], un maléfice tourne autour de ton aura. Je pourrais t'aider aussi."
   "Akame" "Quand dites vous?"
   hide akame
   show punika
   "Punika" "Ce serait un honneur pour moi et la fierté de mon peuple."
   hide punika
   "Elle te regarde fixement."
   "{a=jump:cricri01}Courageux{/a};{a=jump:cricri01}Optimiste{/a};;{a=jump:cricri01}Détérminé{/a};{a=jump:cricri02}Charisme{/a};{a=jump:cricri02}Force{/a};{a=jump:cricri02}intelligence{/a};{a=jump:cricri03}Rien{/a};"
   jump cricri03

label cricri01:
   if courageux == 1:
       "Elle voit votre courage"
       $ attaque += 2
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump cricri2
   if optimiste == 1:
       "Elle voit votre optimiste"
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump cricri2
   if determine == 1:
       "Elle voit votre détermination"
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump cricri2
   "Elle ne voit rien de spécial en vous"
   $ mental -= 40
   if mental <= 2:
       jump crise
   jump cricri2

label cricri02:
   if charisme >= 2:
       "Elle voit votre charisme"
       $ vie += 10
       $ charisme -= 20
       jump cricri2
   if force >= 2:
       "Elle voit votre force"
       $ attaque += 2
       $ force -= 20
       jump cricri2
   if intelligence >= 2:
       "Elle voit votre intelligence"
       $ mental += 10
       $ intelligence -= 20
       if mental >= 100:
           $ mental = 100
       jump cricri2
   "Elle ne voit rien de spécial en vous"
   $ mental -= 50
   if mental <= 2:
       jump crise
   jump cricri2

label cricri03:
   "Elle ne voit rien de spécial en vous"
   $ mental -= 20
   if mental <= 2:
       jump crise
   jump cricri2

label cricri2:
   "Les humains arrivent ..."
   "Vous apercevez Mobos."
   "Une aura menaçante et sombre est en lui"
   "Vous allez directement l'affronter !"
   "Le boss final arrive !"
   play music fight4
   $ res = 0
   jump mobosfight1

label mobosfight1:
   if vie <= 0:
       if res  == 3:
           jump mort
       "Aux portes de la mort, Pakuna vous protège de justesse."
       $ vie = 100
       $ res += 1
   if creature >= 2500:
       "Vous gagnez 100 gold"
       $ gold += 100
       $ creature = 0
       jump mobosfin01
   show mobos
   "Mobos est en fureur et charge droit devant vous."
   "Le combat s'engage."
   if coeur == 1:
       if energie <= 0:
           "Vous n'avez plus d'énergie, vous puisez dans vos ressources mentales."
           $ mental -= 10
           if mental <= 2:
               jump crise
       $ vie += 10
       if vie >= 100:
           $ vie = 100
       $ energie -= 20
       "Grâce à votre technique du cœur vaillant, vous gagnez de la vie."
   if rugissement == 1:
       if energie <= 0:
           "Vous n'avez plus d'énergie. Vous puisez dans vos ressources mentales."
           $ mental -= 10
           if mental <= 2:
               jump crise
       $ attaque += 50
       $ energie -= 20
       "Grâce à votre technique rugissement, vous gagnez 50 attaques."
   if colere == 1:
       if energie <= 0:
           "Vous n'avez plus d'énergie, vous puisez dans vos ressources mentales."
           $ mental -= 10
           if mental <= 2:
               jump crise
       $ creature += renpy.random.randint(1,200)
       $ energie -= 50
       "Grâce à votre technique colère bestiale, vous faite une puissante attaque."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,80)
   "Il vous porte un énorme coup."
   "Il vous reste [vie] vie."
   hide mobos
   "{a=jump:mobosfight1}Attaque{/a}; "
   jump mobosfight1

