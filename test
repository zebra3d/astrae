       if mental <= 0:
           jump crise
       "Veuillez patientez, le démon va bientôt vous recevoir"
       "Cette communication vous sera facturée -20 mental, merci de patienter…"
       "{a=jump:crevassev2}Partir{/a} "
       $ mental -= 20
       if mental <= 0:
           jump crise
     "Veuillez patientez, le démon va bientôt vous recevoir"
       "Cette communication vous sera facturée -20 mental, merci de patienter…"
       "{a=jump:crevassev2}Partir{/a} "
       $ mental -= 20
       if mental <= 0:
           jump crise
       jump crevassev2
   if telephone == "06 69 69 666":
       "Psy Bidule" "Oui bonjour, ici le numéro de téléphone de la célèbre psy Bidule!"
       "Psy Bidule" "Que puis-je faire pour vous?"
       menu psyyy:

           "J'ai des problèmes mentaux.":
               jump psyy

           "J'ai eu des problème avec quelqu'un.":
               jump psy

           "Je crois que je suis amoureux des radiateurs.":
               jump radiateur


label psy:
   "Yae saisit une photo dans sa main."
   show narrateurYae
   "Un visage d'une personne est dessus."
   hide narrateurYae
   "Psy Bidule" "Avec qui avez vous des problèmes?"
   "Joueur" "Avec le narrateur…"
   "Psy Bidule" "Quel est votre prénom?"
   if player_name == "TrouduQ":
       "Psy Bidule" "Je vous met en relation, ne quittez pas."
       jump mondenarrateur
   "Joueur" "Je m'appelle [player_name]"
   "Psy Bidule" "Le voilà le problème."
   "Psy Bidule" "Votre prénom ne sied pas à l'illustre personne qu’est le narrateur!"
   "Psy Bidule" "Je vous propose 2 solutions."
   "Vous suicider ou RevEnir dans l'histoire."
   "{a=jump:mort}Suicide{/a};{a=jump:crevassev2}RevEnir dans l'histoire{/a}"
   jump psy

label mondenarrateur:
   scene muse
   play music opera
   show narrateur
   "Narrateur" "Bien le bonjour [player_name]."
   $ drague = 0
   $ japon = 0
   "Narrateur" "Je désavoue ta venue dans ce modeste habitacle."
   "Narrateur" "En ce jour saint, tu m'importunes et je suis exacerber!"
   "Narrateur" "Voyons voir si t’es un joueur digne de la grande dynastie."
   "Narrateur" "Mon cher [player_name], a tu réussis à trouver les succès de ce jeux?"
   "Narrateur" "Si tu ne les a pas, tu reviendras dans ton aventure grâce à mon illustre pouvoir!"
   hide narrateur
   "{a=jump:crevassev2}Mentir{/a};{a=jump:mort}Mourrir{/a}"
   if succes == 5:
       jump mondenarrateur1
   jump crevassev2

