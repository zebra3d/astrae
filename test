label rocaille:
   "Des grosses roches tombent et vous effleure provoquant un tremblement."
   "Vous apercevez au loin un escalier qui mène sûrement au 2eme étage…"
   "Mais devant cet escalier, une montagne de roche bloque la route."
   "Et s'abat sans cesse devant vous!"
   "Il faut d'abord avancer !"
   "La musique devient de plus en plus rapide."
   "{a=jump:rocaille1}Enthousiaste{/a};{a=jump:rocaille1}Déterminé{/a};{a=jump:rocaille1}Prudent{/a};{a=jump:rocaille1}Courageux{/a};{a=jump:rocaille1}Impulsif{/a};
   {a=jump:rocaille1}Paresseux{/a};{a=jump:rocaille2}Inteligent{/a};{a=jump:rocaille3}Rien..{/a};"
   jump rocaille

label rocaille1:
   if impulsif == 1:
       "Grâce à votre côté impulsif, vous foncez sans hésiter."
       "Au lieu de perdre du mental, vous en gagnez !"
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump rocheo
   if paresseux == 1:
       "A cause de votre côté paresseux, vous vous préparez lentement…"
       $ mental -= 5
       if mental <= 0:
           jump crise
       $ attaque -= 2
       jump rocheo
   if enthousiaste == 1:
       "Grâce à votre enthousiasme, vous reprenez votre route sans broncher."
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump rocheo
   if determine == 1:
       "Grâce à votre détermination, vous réussissez à passer."
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump rocheo
   if prudent == 1:
       "Vous avancez avec prudence."
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump rocheo
   if courageux == 1:
       "Vous n'avez pas peur d'avancer."
       $ mental += 2
       if mental >= 100:
           $ mental = 100
       $ attaque += 2
       jump rocheo
   "Vous n'avez pas les qualités requises pour avancer prudemment."
   $ mental -= 20
   if mental <= 0:
       jump crise
   jump rocheo

