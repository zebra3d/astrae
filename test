label votation:
   "Passons aux votes !"
   $ vote = 0
   $ vote1 = 0
   $ vote2 = 0
   $ vote3 = 0
   $ vote4 = 0
   $ vote5 = 0
   $ vote6 = 0
   $ vote7 = 0
   $ vote8 = 0
   $ vote9 = 0
   $ vote10 = 0
   $ vote += renpy.random.randint(1,10)
   if vote == 1:
       if personne1 == 0:
           jump votation
       $ vote1 += 3
   if vote == 2:
       if personne2 == 0:
           jump votation
       $ vote2 += 3
   if vote == 3:
       if personne3 == 0:
           jump votation
       $ vote3 += 3
   if vote == 4:
       if personne4 == 0:
           jump votation
       $ vote4 += 3
   if vote == 5:
       if personne5 == 0:
           jump votation
       $ vote5 += 3
   if vote == 6:
       if personne6 == 0:
           jump votation
       $ vote6 += 3
   if vote == 7:
       if personne7 == 0:
           jump votation
       $ vote7 += 3
   if vote == 8:
       if personne8 == 0:
           jump votation
       $ vote8 += 3
   if vote == 9:
       if personne9 == 0:
           jump votation
       $ vote9 += 3
   if vote == 10:
       if personne10 == 0:
           jump votation
       $ vote10 += 3
   $ vote = 0
   $ vote = renpy.random.randint(1,10)
   if vote == 1:
       if personne1 == 0:
           jump votation
       $ vote1 += 3
   if vote == 2:
       if personne2 == 0:
           jump votation
       $ vote2 += 3
   if vote == 3:
       if personne3 == 0:
           jump votation
       $ vote3 += 3
