label gablin5:
   show gablin
   "Gablin" "Alors que voulez vous acheter?"
   hide gablin
   "Une lampe,Un casque de protection,Une Boussole,Une Potion de Vie,Une Potion de Mental"
   "{a=jump:article1}Lampe{/a};{a=jump:article2}Casque{/a};{a=jump:article3}Boussole{/a};{a=jump:article10}Puce electronique{/a};{a=jump:article4}Rien{/a}"
   jump gablin5

label article1:
   if lampe == 1:
       "Vous avez déjà une lampe."
       jump gablin5
   if gold <= 49:
       "Vous n'avez pas assez d'argent…"
       jump gablin5
   "Vous êtes sûr de vouloir cette lampe? 50gold{a=jump:gablin5}Non{/a}"
   "Vous avez obtenu une lampe."
   $ lampe = 1
   $ inventaire.append(lampe2)
   $ gold -= 50
   jump gablin5

label article2:
   if casque == 1:
       "Vous avez déjà un casque."
       jump gablin5
   if gold <= 19:
       "Vous n'avez pas assez d'argent…"
       jump gablin5
   "Vous êtes sûr de vouloir un casque? 20gold{a=jump:gablin5}Non{/a}"
   "Vous avez obtenu un casque."
   $ casque = 1
   $ inventaire.append(casque2)
   $ gold -= 20
   jump gablin5

label article10:
   if puce == 1:
       "Vous avez déjà une puce."
       jump gablin5
   if gold <= 29:
       "Vous n'avez pas assez d'argent…"
       jump gablin5
   "Vous êtes sûr de vouloir cette puce électronique à 30 gold {a=jump:gablin5}Non{/a}"
   "Vous avez obtenu une puce électronique."
   $ puce = 1
   $ inventaire.append(puce2)
   $ gold -= 30
   jump gablin5

