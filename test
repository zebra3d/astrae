label narrateurtenebre:
   show narrateurshadow
   with vpunch
   if vie <= 0:
       jump mort
   if creature >= 500:
       hide narrateurshadow
       show narrateuraie
       "Narrateur" "Et merde… cette fois si j'ai tout mi dans les pv et pratiquement rien en attaque."
       hide narrateuraie
       play music zombie
       jump narrateurzombie
   play music darkfight
   "Le Narrateur se dresse devant vous."
   "Le combat s'engage."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,5)
   "Le Narrateur vous porte 1d5 dégât."
   "Vous envoi un screamer."
   show peur
   play sound scary
   "BOUH!"
   hide peur
   "Il vous reste [vie] vie."
   hide narrateurshadow
   "{a=jump:narrateurtenebre}frapper encore{/a} ; "
   jump narrateurtenebre

