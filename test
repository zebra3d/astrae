label finvote4:
   "Alors voyons les votes."
   $ vote = 0
   $ egalite = 0
   "[vote1] vote contre Mioru."
   "[vote2] vote contre Scara."
   "[vote3] vote contre Jinks."
   "[vote4] vote contre Yui."
   "[vote5] vote contre Elayna."
   "[vote6] vote contre Soraya."
   "[vote7] vote contre Seikyo."
   "[vote8] vote contre Rubie."
   "[vote9] vote contre Dimitri."
   "[vote10] vote contre [player_name]."
label Mioki003:
   if vote1 >= vote2:
       if vote1 == vote2:
           jump Scara003
       if vote1 >= vote3:
           if vote1 == vote3:
               jump Scara003
           if vote1 >= vote4:
               if vote1 == vote4:
                   jump Scara003
               if vote1 >= vote5:
                   if vote1 == vote5:
                       jump Scara003
                   if vote1 >= vote6:
                       if vote1 == vote6:
                           jump Scara003
                       if vote1 >= vote7:
                           if vote1 == vote7:
                               jump Scara003
                           if vote1 >= vote8:
                               if vote1 == vote8:
                                   jump Scara003
                               if vote1 >= vote9:
