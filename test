label atanos1:
   if impulsif == 1:
       "A cause de votre impulsivité, vous perdez votre courage."
       $ mental -= 20
       if mental <= 2:
           jump crise
       jump atanosfight
   if arrogant == 1:
       "A cause de votre arrogance, vous perdez du mental."
       $ mental -= 20
       if mental <= 2:
           jump crise
       jump atanosfight
   if colerique == 1:
       "A cause de votre colère, vous perdez tous vos moyens."
       $ mental -= 20
       $ attaque += 2
       if mental <= 2:
           jump crise
       jump atanosfight
   if efficace == 1:
       "Grâce à votre efficacité, vous êtes prêt à l'affronter."
       $ mental += 10
       if mental == 100:
           $ mental = 100
       jump atanosfight
   if fiable == 1:
       "Grâce à votre fiabilité, vous croyez en vos paroles."
       $ mental += 10
       if mental == 100:
           $ mental = 100
       jump atanosfight
   if determine == 1:
       "Grâce à votre détermination, vous êtes prêt à l'affronter."
       $ mental += 10
       if mental == 100:
           $ mental = 100
       jump atanosfight
   if agile == 1:
       "Grâce à votre forte agilité vous ne tremblez pas."
       $ mental += 10
       if mental == 100:
           $ mental = 100
       jump atanosfight
   if courageux == 1:
       "Grâce à votre courage, vous êtes prêt à l'affronter."
       $ mental += 10
       $ attaque += 2
       if mental == 100:
           $ mental = 100
       jump atanosfight
   if optimiste == 1:
       "Grâce à votre talent, vous restez optimiste."
       $ mental += 10
       if mental == 100:
           $ mental = 100
       jump atanosfight
   "Vous avez triché, par conséquent vous perdez -40 mental."
   $ mental -= 40
   if mental <= 2:
       jump crise
   jump atanosfight

label atanos2:
   if force >= 2:
       "Grâce à votre force, vous êtes prêt à faire face au danger !"
       $ force -= 20
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       $ attaque += 2
       jump atanosfight
   "Vous n'avez pas de force, les conséquences de cette tentative de triche vous coûte 50 de mental."
   $ mental -= 50
   if mental <= 2:
       jump crise
   jump atanosfight

label atanos3:
   "Votre respiration s'envenime et devient trop forte, elle comprime vos poumons."
   "Vous perdez 20 mental."
   $ mental -= 20
   if mental <= 2:
       jump crise
   jump atanosfight

label atanosfight:
   if vie <= 10:
       jump atanosfin
   if creature >= 300:
       "Vous gagnez 50 gold."
       $ gold += 50
       $ creature = 0
       jump atanosfin
   play music fight3
   show atanos
   "Atanos dégaine son épée et charge droit devant vous."
   "Le combat s'engage."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,25)
   "Il vous porte un énorme coup."
   "Il vous reste [vie] vie."
   hide atanos
   "{a=jump:atanosfight2}Parade{/a}; {a=jump:atanosfight1}Attaque{/a};  "
   jump atanosfight

