label jardin:
   if cleptoman == 1:
       "Vous trouvez une rose."
       "Vous décidez de rejoindre les autres."
       $ mental -= 10
       if mental <= 0:
           jump crise
       $ rose = 1
       $ inventaire.append(rose2)
       jump cale
   if herboriste == 1:
       "Vous farfouillez dans les plantes et vous trouver,"
       "Une Rose."
       "Vous décidez de rejoindre les autres."
       $ mental += 5
       if mental >= 100:
           $ mental = 100
       $ vie += 5
       if vie >= 100:
           $ vie = 100
       $ rose = 1
       $ inventaire.append(rose2)
       jump cale
   if farfouilleur == 1:
       "Vous farfouillez dans les plantes et vous trouvez,"
       "Une Rose."
       "Vous décidez de rejoindre les autres."
       $ mental += 2
       if mental >= 100:
           $ mental = 100
       $ rose = 1
       jump cale
   "Vous regardez attentivement, l'odeur que dégage les plantes vous fait tourner la tête…"
   $ mental -= 20
   if mental <= 0:
       jump crise
   "Vous perdez -20 mental."
   "Vous décidez de rejoindre les autres."
   jump cale

