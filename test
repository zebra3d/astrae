label combatclone:
   if vie <= 0:
       jump mort
   if creature >= 100:
       "Vous gagnez 100gold"
       $ gold += 100
       $ creature = 0
       jump clonefin
   show archer
   "[player_name] et en fureur et charge droit devant vous"
   "Le combat s'engage"
   if coeurvaillant == 1:
       $ vie += 10
       if vie >= 100:
           $ vie = 100
       $ energie -= 20
       "Grace a votre technique du coeur vaillant vous gagnez de la vie"
   if tranchelame == 1:
       $ creature += renpy.random.randint(1,20)
       $ creature += renpy.random.randint(1,20)
       $ creature += renpy.random.randint(1,20)
       $ energie -= 10
       "Grace a votre technique tranche lame vous faite 3 coup tranchant d'épée"
   if briseetoile == 1:
       $ creature += renpy.random.randint(1,100)
       $ energie -= 50
       "Grace a votre technique brise étoile vous faite une puissante attaque"
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,20)
   "il vous porte un enorme coup"
   "Il vous reste [vie] vie."
   hide archer
   "{a=jump:combatclone}Attaque{/a}; "
   jump combatclone

label clonefin:
   stop music
   play music helios
   "Après de lourd dégat subit c'est comme si votre ame avez récupéré un morceau de vous même"
   "Au loin vous appercevez mobos"
   show mobos
   "Mobos" "Tous contre l'ange"
   hide mobos
   "C'est mots prononcé par mobos vous entraine face a l'ange akame"
   "Une puissante lumiere jaillit de ces main et extermine la plupart des humain"
   "Vous remarquer que malheureusement kyrele,Augustus,Alexandre,Alexandra et penelope gravement blesser"
   "l'ange s'apprête de nouveau a faire une attaque très puissante"
   "C'est la que les cinq chevalier sinterpose et ce place devant vous"
   show royal5
   "Penelope" "A toi de jouer [player_name] on compte sur toi"
   hide royal5
   show royal1
   "Kyrele" "Ramene moi une petite piece sur mon cerceuil [player_name]"
   hide royal1
   show royal2
   "Augustus" "Augustus se rappellera toujours de votre nom"
   hide royal2
   show royal3
   "Alexandre" "Je me sacrifie comme un hero pour montré a ma soeur que j'aurais fait un bon mari"
   hide royal3
   show royal4
   "Alexandra" "Il et temps pour toi de CROA CROA en toi"
   hide royal4
   "Puis un déchainement de lumiere"
   "Les corps de plusieurs centaine de chevalier aglutiner devant vous"
   "Et la mort des cinq chevalier"
   "Vous n'avez que une envie c'est de vous venger"
   show Yae
   "Yae" "Je vais t'aider [player_name] parcontre je ne pourrais pas le faire beaucoup de fois alors vazy fonce"
   hide Yae
   "Le combat contre l'ange arrive voici le boss final !"
   $ res = 0
   play music fight4
   jump combatange

label combatange:
   if vie <= 0:
       if res  == 3:
           jump mort
       "Au porte de la mort Yae vous soigne de justesse"
       $ vie = 100
   if creature >= 2500:
       "Vous gagnez 100gold"
       $ gold += 100
       $ creature = 0
       jump angefin
   show akame
   "[player_name] et en fureur et charge droit devant vous"
   "Le combat s'engage"
   if coeurvaillant == 1:
       if energie <= 0:
           "Vous n'avez plus d'énergie vous puisez dans vos ressource mental"
           $ mental -= 10
           if mental <= 2:
               jump crise
       $ vie += 10
       if vie >= 100:
           $ vie = 100
       $ energie -= 20
       "Grace a votre technique du coeur vaillant vous gagnez de la vie"
   if tranchelame == 1:
       if energie <= 0:
           "Vous n'avez plus d'énergie vous puisez dans vos ressource mental"
           $ mental -= 10
           if mental <= 2:
               jump crise
       $ creature += renpy.random.randint(1,20)
       $ creature += renpy.random.randint(1,20)
       $ creature += renpy.random.randint(1,20)
       $ energie -= 10
       "Grace a votre technique tranche lame vous faite 3 coup tranchant d'épée"
   if briseetoile == 1:
       if energie <= 0:
           "Vous n'avez plus d'énergie vous puisez dans vos ressource mental"
           $ mental -= 10
           if mental <= 2:
               jump crise
       $ creature += renpy.random.randint(1,200)
       $ energie -= 50
       "Grace a votre technique brise étoile vous faite une puissante attaque"
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,80)
   "il vous porte un enorme coup"
   "Il vous reste [vie] vie."
   hide akame
   "{a=jump:combatange}Attaque{/a}; "
   jump combatange

