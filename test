label achat1:
   if amical == 1:
       "Le renard vous trouve très amical, il a l'air de vous proposer un marché."
       "Vous gagnez + 2 mental."
       $ mental += 2
       jump troc
   if fiable == 1:
       "Vous regardez le renard, il vous trouve fiable."
       "Vous gagnez += 10 mental."
       $ mental += 10
       jump troc
   if bete == 1:
       "Le renard vous parle."
       "Renard" "Bonjour! je cherche quelqu'un à qui vendre ces objet."
       "Renard" "Vous avez du gold?"
       $ mental += 2
       "Vous gagnez +2 mental."
       jump troc
   "Le renard vous observe avec mépris."
   "Vous perdez -20 mental."
   "Le renard s'enfuit."
   $ mental -= 20
   jump mono

label achat2:
   if zoophile == 1:
       "Vous essayez de violer le renard."
       "Il s'enfuit"
       "Vous perdez -20 mental."
       $ mental -= 20
       jump mono
   "Le renard vous observe avec mépris."
   "Vous perdez -20 mental."
   "Le renard s'enfuit."
   $ mental -= 20
   jump mono

label achat3:
   if charisme >= 2:
       "Vous essayez de comprendre le renard."
       $ charisme -= 10
       "Il a envie de vous vendre quelque chose."
       jump troc
   "Le renard vous observe avec mépris."
   "Vous perdez -40 mental."
   "Le renard s'enfuit."
   $ mental -= 40
   jump mono

label troc:
   "Le renard vous regarde et vous montre 3 objets."
   "Un couteau à 20 gold ; Une Potion de Soin à 10 gold ; Une Potion de mental à 10 gold ; Une Rune d'Attaque a 10 gold."
   "{a=jump:renard1}Econome{/a} ; {a=jump:renard1}Magnifique{/a} ; {a=jump:renard2}Moche{/a} ; {a=jump:renard2}Cleptoman{/a} ; {a=jump:renard3}intelligence{/a} ; {a=jump:renard}Je vais voir..{/a}"
   jump troc

label renard1:
   if econome == 1:
       "Le renard fait un geste commercial, il vous donne 10 gold."
       $ gold += 10
       $ mental += 2
       jump renard
   if magnifique == 1:
       "Le renard vous regarde avec tendresse."
       $ mental += 10
       jump renard
   "Le renard vous observe avec mépris."
   "Vous perdez -20 mental."
   "Le renard s'enfuit."
   $ mental -= 20
   jump mono

label renard2:
   if moche == 1:
       "Le renard vous regarde avec mépris."
       $ mental -= 5
       "Vous perdez - 5 mental."
       "Le renard vous prend 5 gold."
       $ gold -= 5
       jump renard
   if cleptoman == 1:
       "Vous perdez -10 mental."
       "Vous avez récupéré une potion de soin."
       "Vous l'utilisez."
       $ vie += 20
       "Elle vous rend 20 pv."
       "Le renard s'enfuit !"
       jump mono
   "Le renard vous observe avec mépris."
   "Vous perdez -20 mental."
   "Le renard s'enfuit."
   $ mental -= 20
   jump mono

label renard3:
   if intelligence >= 2:
       "Le renard vous donne une potion de mental."
       "Vous l'utilisez."
       "Vous gagnez +20 mental."
       $ mental += 20
       $ intelligence -= 10
       jump renard
   "Le renard vous observe avec mépris."
   "Vous perdez -40 mental."
   "Le renard s'enfuit."
   $ mental -= 40
   jump mono

label renard:
   "Qu'allez vous acheter?"
   "{a=jump:couteau}Couteau{/a} ; {a=jump:p1}Potion de Soin{/a} ; {a=jump:p2}Potion de Mental{/a} ; {a=jump:mono}Ne rien acheter{/a}"
   jump renard

label couteau:
   if couteau == 1:
       "Vous avez déjà un couteau."
       jump renard
   if gold <= 19:
       "Vous n'avez plus assez d'argent…"
       jump renard
   "Vous achetez un couteau, pas sur de savoir pourquoi… vous avez déjà une arme…"
   "Vous avez reçu un couteau !"
   $ gold -= 20
   $ couteau = 1
   $ inventaire.append(couteauu)
   jump renard

label p1:
   if gold <= 9:
       "Vous n'avez plus assez d'argent…"
       jump renard
   "Vous achetez une potion de soin, vous gagnez 20 pv."
   $ gold -= 10
   $ vie += 20
   jump renard

label p2:
   if gold <= 9:
       "Vous n'avez plus assez d'argent."
       jump renard
   "Vous achetez une potion de mental, vous gagnez + 20 mental."
   $ gold -= 10
   $ mental += 20
   jump renard

label mono:
   scene decorsmonolyte
   "Tu aperçoit de géant monolythes des forêts."
   "???" "TU N’A RIEN A FAIRE SUR CETTE TERRE!"
   "???" "Soudain les plantes commencent à bouger."
   "???" "De grandes lianes se forment et commencent à saisir ton corps."
   "???" "Vous ne pouvez plus bouger."
   "{a=jump:ronce1}Si vous avez un couteau{/a} ; {a=jump:ronce2}Agile{/a} ; {a=jump:ronce2}Herboriste{/a} ; {a=jump:ronce2}Creatif{/a} ; {a=jump:ronce3}Force{/a} ; {a=jump:ronce2}Je ne peux rien faire !{/a}"
   jump mono

label ronce1:
   if couteau == 1:
       "Vous prenez le couteau qui vous permet de couper ces lianes..."
       "Vous êtes libre désormais."
       $ couteau = 0
       $ inventaire.remove(couteauu)
       jump fleur
   "Vous n'avez pas de couteau, vous ne pouvez rien faire."
   "Les lianne se serrent et vous êtes en train d'être prisonnier d'une étreinte mortelle."
   jump mort

label ronce2:
   if agile == 1:
       "Vous réussissez à vous dégager de l'étreinte."
       $ mental += 10
       "Vous gagnez 10 mental."
       jump fleur
   if herboriste == 1:
       "Grâce à votre connaissance des plantes, vous réussissez à vous dégager."
       $ mental += 5
       "Vous gagnez 5 mental, mais vous ne récupérez pas de vie."
       jump fleur
   if creatif == 1:
       "Grâce à votre créativité, vous créez un objet tranchant à l'aide de votre arme."
       "Vous réussissez à couper les lianes."
       $ mental += 15
       "Vous gagnez 15 mental."
       jump fleur
   "Les lianes se serrent et vous êtes en train d'être prisonnier d'une étreinte mortelle."
   jump mort

label ronce3:
   if force >= 2:
       "Grâce à votre force, vous réussissez à vous dégager des ronces."
       $ force -= 10
       jump fleur
   "Les lianne se serrent et vous êtes en train d'être prisonnier d'une étreinte mortelle."
   jump mort

label fleur:
   "???" "Vous avez réussi à vous dégager de notre étreinte."
   "???" "Nous allons informer la grande maîtresse des forêts."
   "???" "Elle vous terrassera !!"
   "???" "Vous avez osez vous aventurer dans l'équilibre de la terre."
   "???" "Nous ne tolérons pas un tel affront!"
   "???" "Vous allez le payer!"
   "Les monolythes partent prévEnir leur maître."
   "Tu t'enfonces dans leur territoire et tu trouves une fleur."
   "Cette fleur est noir avec une aura obscure…"
   "Que faites-vous?"
   $ fleur = 0
   "{a=jump:plante}Herboriste{/a} ; {a=jump:plante1}La Prendre{/a} ; {a=jump:plante2}Ne pas la prendre{/a}"
   jump fleur

label plante:
   if herboriste == 1:
       "Vous inspectez la fleur grâce à votre connaissance des plantes."
       "Cette fleur vous fait perdre du mental, mais vous fait gagner de l'attaque."
       "Cette fleur a l'air importante… elle est la seule qu'il y a ici."
       "C'est une fleur d'étoile."
       $ mental += 5
       $ vie += 5
       "Vous gagnez +5 mental et +5 pv."
       "{a=jump:plante1}La Prendre{/a} ; {a=jump:plante2}Ne pas la prendre{/a}"
       jump plante1

label plante1:
   "Vous décidez de prendre la plante."
   "Elle vous donne un grand vertige."
   "Vous perdez -20 mental."
   $ mental -= 20
   "Vous vous sentez plus fort."
   $ attaque += 10
   $ fleur = 1
   jump plantation

label plante2:
   "Vous décidez de ne pas la prendre."
   "Un grand soulagement vous dit que vous avez fait le bon choix!"
   "Vous gagnez +20 mental."
   $ mental += 20
   jump plantation

label plantation:
   scene combatmonolytegeant
   "???" "TOI"
   "???" "Je suis Aloy, Gardienne des forêts."
   "Aloy" "Que fais-tu sur notre terre?"
   h "Je suis la parce que vous empêchez les races de rentrer ici."
   h "Vous les avez attaqué et blessé."
   "Aloy" "Cette forêt doit être protégée! nous n'avons pas d'autre choix!"
   "Aloy" "On va faire un accord, si tu réussis à me battre, je te proposerais une solution."
   "{a=jump:aloy1}Accepter{/a} ; {a=jump:aloy2}Refuser{/a}"
   jump plantation

label aloy1:
   "Vous acceptez de combattre Aloy."
   "Mais elle a l'air très forte… elle est immense comparé à vous."
   "Aloy" "Je vais vous donner un avantage, allez vous l'acceptez?"
   "{a=jump:aloy3}Fiable{/a} ; {a=jump:aloy3}Persuasif{/a} ; {a=jump:aloy4}Timide{/a} ; {a=jump:aloy4}Moche{/a} ;{a=jump:aloy4}Immature{/a} ; {a=jump:aloy5}Charismatique{/a} ; {a=jump:aloy5}intelligence{/a} ; {a=jump:aloy5}Force{/a} ; {a=jump:combataloy}Je ne peux pas!{/a}"
   jump aloy1

label aloy2:
   "Vous ne pouvez pas accepter, elle a l'air beaucoup trop forte pour vous!"
   "Vous déclinez poliment et vous allez revoir Bladin."
   h "Je suis désolé… je n'ai pas pu réussir à les arrêter… il continueront de vous attaquer."
   "Bladin" "C'est pas grave, vous avez fait de votre mieux… voilà 10 gold pour vous remercier !"
   "Vous dites au revoir à Bladin."
   jump crevassev2

label aloy3:
   if fiable == 1:
       "Vous êtes quelqu'un de fiable."
       "Voici pour vous, une arme faite à partir de bois céleste."
       "Prenez-la pour le combat, vous pourrez la garder si vous gagnez."
       $ attaque += 40
       $ arme = 1
       "Vous gagnez + 5 mental."
       $ mental += 5
       jump combataloy
   if persuasif == 1:
       "Vous êtes quelqu'un de persuasif."
       "Voici pour vous, une arme faite à partir de bois céleste."
       $ attaque += 40
       $ arme = 1
       jump combataloy
   "Vous n'avez pas les qualités requises finalement !"
   "Vous perdez -20 mental."
   $ mental -= 20
   jump combataloy

label aloy4:
   if timide == 1:
       "Vous êtes trop gêné pour accepter un geste, vous refusez poliment…"
       "Vous perdez - 5 mental."
       $ mental -= 5
       jump combataloy
   if moche == 1:
       "Aloy" "Finalement, je vais revoir mon avis!"
       $ mental -= 5
       "Vous perdez - 5 mental."
       jump combataloy
   if immature == 1:
       "Vous êtes trop immature pour recevoir quoi que ce soit !"
       $ mental -= 5
       "Vous avez perdu - 5 mental."
       jump combataloy
   "Vous n'avez pas les qualités requises finalement !"
   "Vous perdez -20 mental."
   $ mental -= 20
   jump combataloy

label aloy5:
   if intelligence >= 2:
       "Aloy" "Une tête de noeud va bien prendre une arme."
       "Aloy" "Voici une arme construite à partir de bois céleste."
       $ arme = 1
       $ intelligence -= 10
       $ attaque += 40
       jump combataloy
   if charisme >= 2:
       "Voici une arme construite à partir de bois céleste."
       $ arme = 1
       $ charisme -= 10
       $ attaque += 40
       jump combataloy
   if force >= 2:
       "Aloy" "Quand je vois votre carrure, je pense que vous serez un valeureux adversaire… alors voici pour vous, pour que le combat soit pimenté!"
       $ arme = 1
       $ force -= 10
       $ attaque += 40
       jump combataloy
   "Vous n'avez pas les qualités requises finalement !"
   "Vous perdez -40 mental."
   $ mental -= 40
   jump combataloy

label combataloy:
   if vie <= 0:
       jump mort
   if monstre >= 100:
       play music foret
       "Vous gagnez 250 gold."
       $ gold += 250
       $ monstre = 0
       jump aloyreussi
   play music fight
   "Aloy se dresse devant vous."
   "Le combat s'engage."
   $ monstre += renpy.random.randint(1,20)
   $ monstre += attaque
   "Vous portez un coup d'attaque de [monstre]."
   $ vie -= renpy.random.randint(1,125)
   "Elle vous porte un énorme coup."
   "Il vous reste [vie] vie.."
   "{a=jump:combataloy}frapper encore{/a}  "
   jump combataloy

label aloyreussi:
   "Aloy" "Vous avez réussi !"
   "Aloy" "Je n'ai jamais eu d'aussi grand adversaire !"
   "Aloy" "Bon, je vous accorde un choix, j'ai une solution à votre problème! "
   "Aloy" "Soit vous êtes contre nous, soit vous êtes avec nous."
   "{a=jump:avec}Avec vous{/a} ; {a=jump:contre}Contre vous{/a} "
   jump aloyreussi

label avec:
   "Vous décidez de faire confiance à Aloy"
   "Aloy" "Il se trouve que nous avons une fleur."
   "Aloy" "Cette fleur atteindra sa maturité cette nuit."
   "Aloy" "C'est une fleur étoile."
   "Aloy" "Elle reste sombre pendant tout un certain temps."
   "Aloy" "Mais dès qu'elle atteint sa maturité et que les étoiles fileront dans le ciel,"
   "Aloy" "Un arbre divin sera créé, ce qui empêchera toute personne de vEnir."
   "Aloy" "Et donc nous éviteront le combat."
   "Aloy" "Cette solution vous convient?"
   "{a=jump:eclat}Oui{/a} ; {a=jump:contre}Non{/a} "
   jump avec

label eclat:
   if fleur == 1:
       if herboriste == 1:
           "Aloy" "Je ne comprends pas, cette fleur était ici!"
           h "C'est moi qui l’ai prise."
           "Aloy" "Il fallait la laisser planté."
           "Aloy" "Pour qu’elle puisse éclore!"
           h "Ne vous inquiétez pas, grâce à mes compétences avec les plantes, elle est bien conservée!"
           h "Je vais la replanter, ça devrait suffire pour qu’elle éclot pour cette nuit."
           "Aloy vous remercie d'avoir pris soin de la fleur."
           "Elle vous offre un morceau d'étoile."
           $ etoile += 1
           $ inventaire.append(etoilee)
           "Aloy" "Grâce à ce morceau d'étoile, vous guérirez rapidement de vos blessures."
           "Aloy" "Et qui sait, ça pourrait vous servir dans votre aventure."
           "Aloy" "Alors, voulez-vous planter la fleur d'étoile pour que la paix soit instaurée?"
           "{a=jump:finmonolyte}Oui{/a} ; {a=jump:pladin}Non{/a} "
           jump eclat
       "Aloy" "Je ne comprend pas… cette fleur était ici!"
       h "C'est moi qui l’ai prise."
       "Aloy" "Il fallait la laisser planté!"
       "Aloy" "Pour qu'elle puisse éclore!"
       "Aloy vous fait signe de repartir chez vous."
       jump pladin
   "Aloy" "C’est bon elle est toujours là."
   "Aloy" "Alors, voulez-vous accomplir cette tâche pour que la paix soit instaurée?"
   "{a=jump:finmonolyte}Oui{/a} ; {a=jump:pladin}Non{/a} "
   jump eclat

label finmonolyte:
   scene black
   play music finforet
   "Vous attendez que les étoiles tombent et soudain."
   "La fleur commence à donner naissance à un arbre."
   show finmonolyte
   "Cet arbre céleste permettra de garantir une protection stable pendant quelques années…"
   "Aloy" "Nous sommes en sécurité maintenant! nous n'aurons plus à nous battre!"
   "Aloy" "Merci brave guerrier!"
   "Aloy" "Dites moi votre nom !"
   h "Je m'appelle [player_name]."
   "Aloy" "Je tâcherai de m'en souvenir !"
   "Vous récupérez un morceau de souvenir."
   "{b}*Fleur d'Etoile*{/b}"
   "Notez bien les souvenirs que vous récupérez. "
   "Vous avez réussi à maintenir une paix durable !"
   hide finmonolyte
   return

label pladin:
   scene decorsbois
   "Vous retournez voir Bladin."
   "En lui disant que vous n'avez pas réussi à vous mettre d'accord avec Aloy."
   "Il vous remercie et vous donne 20 gold."
   $ gold += 20
   jump crevassev2

label contre:
   h "Je suis désolé, mais je ne peux tolérer vos actes! "
   h "Malgré la protection que vous voulez instaurer, vous avez fait des morts et des blessés!"
   h "Par conséquent, je me dois de refuser."
   "Aloy" "Très bien, alors vu que vous ne me faites pas confiance…"
   "Aloy" "Vous pouvez repartir, brave guerrier."
   "Aloy" "Nous continueront a instauré la protection en attaquant quiconque."
   "Allez voir Bladin pour lui rapporter la nouvelle."
   "Vous retournez donc voir Bladin dans la forêt."
   scene decorsbois
   h "Je n'ai pas réussi à trouver de solution avec la chef."
   h "Je suis vraiment désolé…"
   "Bladin" "Ce n'est pas grave."
   "Bladin" "Tenez, voici pour vous remercier !"
   "Il vous donne 20 gold."
   $ gold += 20
   jump crevassev2


label desert:
   scene  decorsdesert
   play music desert
   "Vous vous dirigez donc dans le désert. "
   "Un homme vous interpelle…"
   show Bunda
   $ arene = 0
   "???" "Vous la !"
   "???" "Vous avez l'air d'être un valeureux guerrier!"
   "???" "Je m'appelle Bunda! je suis un organisateur de combat."
   "Bunda" "Je vous propose d'affronter des monstres."
   "Bunda" "Nous vous offrons une récompense à chaque monstre tué."
   "Bunda" "Bien sûr, à la fin, je vous réserve une petite surprise !"
   hide Bunda
   h "D'accord, j'accepte !"
   show Bunda
   "Bunda" "Bien, avant le combat, avez vous besoin de quelque chose en particulier?"
   "Bunda" "Je vous accorde 1 objet gratuit si vous avez les moyens de me convaincre !"
   "{a=jump:Bunda1}Fiable{/a} ; {a=jump:Bunda1}Drole{/a} ; {a=jump:Bunda1}Econome{/a}; {a=jump:Bunda1}Courageux{/a} ; {a=jump:Bunda2}Colérique{/a} ; {a=jump:Bunda2}Déprimé{/a} ; {a=jump:Bunda3}Charismatique{/a} ; {a=jump:Bunda3}Inteligent{/a} ; {a=jump:Bunda3}Force{/a}  ; {a=jump:Bunda4}Je n'ai pas d'argument..{/a}"
   jump desert

label Bunda1:
   if fiable == 1:
       "Bunda" "Je vois que vous êtes assez fiable."
       "Bunda" "Choisissez un objet!"
       $ mental += 10
       "Vous gagnez 10 mental."
       jump armement
   if drole == 1:
       h "J'aime quand sara corde les gens…"
       "Bunda" "Vous êtes un sacré farceur!"
       "Choisissez un objet"
       "Vous gagnez +15 mental."
       $ mental += 15
       jump armement
   if econome == 1:
       "Vous savez comment marchander avec Bunda."
       "Il vous donne +10 gold."
       "Choisissez un objet!"
       $ gold += 10
       $ mental += 2
       jump armement
   if courageux == 1:
       "Vous n'avez pas peur du danger !"
       $ attaque += 2
       $ mental += 5
       "Vous gagnez +2 attaque et +5 mental."
       "Choisissez un objet"
       jump armement
   "Vous n'avez pas les caractéristiques que je recherche pour le combat…"
   "Vous êtes vexé, vous perdez -20 mental pour avoir triché."
   $ mental -= 20
   jump armement

