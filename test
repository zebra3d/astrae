   show atanos
   "Atanos" "Profite en aussi pour allez voir un marchand et de t'acheter un équipement plus puissant"
   hide atanos
   show gobelin
   "Gobelin" "Tien tien on c'est pas déjà vue quelque part ? votre visage me semble familier"
   "Gobelin" "Bon écouter je suis le marchand la ou il y'a des pigeon je suis la pour les plumer"
   "Gobelin" "Ne vous inquiété pas je ne parle pas de vous sa parrait evident il suffit de voir votre allure"
   "Gobelin" "Vous êtes un prestigieux aventurier !"
   hide gobelin
   show atanos
   "Atanos" "Tien mon fidele serviteur ajoute donc c'est objets a ta vente je suis sur qu'il pourront intéréssé [player_name]"
   hide atanos
   show gobelin
   "Gobelin" "Mais maitre c'est objet sont d'une grande valeur !"
   "Gobelin" "Je ne sais comment vous remercier merci mon maitre"
   hide gobelin
   show atanos
   "Atanos" "Fait lui un prix résonable mon serviteur"
   hide atanos
   show gobelin
   "Gobelin" "Bien mon maitre"
   hide gobelin
   "Alors voici mais article a vendre"
   jump ventegobelin

label ventegobelin:
   show potiondesoin
   "Une potion de soin a 20gold"
   hide potiondesoin
   show potiondesoin
   with vpunch
   "Une Grande potion de soin a 40gold"
   hide potiondesoin
   show potiondemental
   "Une potion de mental a 20 gold"
   hide potion de mental
   show potiondemental
   with vpunch
   "Une Grande Potion de mental a 40 gold"
   hide potiondemental
   show baguemental
   "Une Bague de mantal a 80 gold"
   hide baguemental
   if guerrier == 1:
       show epeedelumiere
       "Une épée de Lumiere a 100gold"
       hide epeedelumiere
   if assassin == 1:
       show daguedelumiere
       "Une Dague de Lumiere a 100 gold"
       hide daguedelumiere
   if mage == 1:
       show livredelumiere
       "Un livre de lumiere a 100gold"
       hide livredelumiere
   if rodeur == 1:
       show arcdelumiere
       "Un Arc de lumiere a 100gold"
       hide arcdelumiere
   "Une rune de pouvoir a 40 gold"
   "Alors que choisit tu?"
   "{a=jump:pv01}PotionDeVie{/a};{a=jump:pv02}GrandePotionPV{/a};{a=jump:pm01}PotiondePM{/a};{a=jump:pm02}GrandePotiondePM{/a};{a=jump:pm03}BaguedeMental{/a};{a=jump:arm01}Arme{/a};{a=jump:runee01}Rune{/a};{a=jump:cristal01}Rien{/a};"
   jump ventegobelin

label pv01:
   if gold <= 19:
       "Vous n'avez pas assez d'argent"
       jump ventegobelin
   "Vous avez acheter une potion de vie"
   $ vie += 20
   $ gold -= 20
   "Vous regagnez immédiatement 20point de vie"
   if vie >= 100:
       $ vie = 100
   jump ventegobelin

label pv02:
   if gold <= 39:
       "Vous n'avez pas assez d'argent"
       jump ventegobelin
   "Vous acheter une Grande potion de vie"
   $ vie += 50
   $ gold -= 40
   "Vous regagnez immédiatement 50pv"
   if vie >= 100:
       $ vie = 100
   jump ventegobelin

label pm01:
   if gold <= 19:
       "Vous n'avez pas assez d'argent"
       jump ventegobelin
   "Vous avez acheter une potion de mental"
   $ mental += 20
   $ gold -= 20
   "Vous regagnez immédiatement 20point de mental"
   if mental >= 100:
       $ mental = 100
   jump ventegobelin

label pm02:
   if gold <= 39:
       "Vous n'avez pas assez d'argent"
       jump ventegobelin
   "Vous avez achetez une grande potion de mental"
   $ mental += 50
   $ gold -= 40
   if mental >= 100:
       $ mental = 100
   jump ventegobelin

label pm03:
   if gold <= 79:
       "Vous n'avez pas assez d'argent"
       jump ventegobelin
   "Vous avez acheter une bague de mental"
   $ mental = 100
   $ gold -= 80
   jump ventegobelin


label arm01:
   if gold <= 99:
       "Vous n'avez pas assez d'argent"
       jump ventegobelin
   "Vous avez acheter une arme de lumiere"
   $ attaque += 150
   $ gold -= 100
   jump ventegobelin

label runee01:
   if gold <= 39:
       "Vous n'avez pas assez d'argent"
       jump ventegobelin
   "Vous avez acheter une rune celeste"
   $ attaque += 50
   $ gold -= 50
   jump ventegobelin

label cristal01:
   "Maintenant vous êtes prêt a continuer l'aventure"
   "Qu'est ce que qui vous attendra sur le chemin?"
