label combatclone1:
   if vie <= 0:
       jump mort
   if creature >= 100:
       "Vous gagnez 100 gold."
       $ gold += 100
       $ creature = 0
       jump clonefin1
   show guerrier
   "[player_name] est en fureur et charge droit devant vous."
   "Le combat s'engage."
   if coeur == 1:
       $ vie += 10
       if vie >= 100:
           $ vie = 100
       $ energie -= 20
       "Grâce à votre technique du cœur vaillant, vous gagnez de la vie."
   if rugissement == 1:
       if energie <= 0:
           "Vous n'avez plus d'énergie, vous puisez dans vos ressources mentales."
           $ mental -= 10
           if mental <= 2:
               jump crise
       $ attaque += 50
       $ energie -= 20
       "Grâce à votre technique rugissement, vous gagnez 50 attaque"
   if colere == 1:
       if energie <= 0:
           "Vous n'avez plus d'énergie, vous puisez dans vos ressources mentales."
           $ mental -= 10
           if mental <= 2:
               jump crise
       $ creature += renpy.random.randint(1,200)
       $ energie -= 50
       "Grâce à votre technique colère bestiale, vous faite une puissante attaque."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,20)
   "Il vous porte un énorme coup."
   "Il vous reste [vie] vie."
   hide guerrier
   "{a=jump:combatclone1}Attaque{/a}; "
   jump combatclone1

