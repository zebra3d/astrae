label jeux1:
   if roulettejoueur >= 15:
       show safia
       "Safia" "Bravo tartiflette! tiens voila ce que j'ai trouvé."
       hide safia
       "Elle vous tend une boite de cigare."
       $ cigare = 1
       $ inventaire.append(cigare2)
       "Vous décidez de rejoindre les autres."
       jump cale
   if roulettesafia >= 10:
       "Vous avez perdu 10 gold."
       $ gold -= 10
       show safia
       "Safia" "Fais pas cette tête Tartiflette… tu gagnera bien un jour!"
       hide safia
   "Vous lancez la partie !"
   $ roulettejoueur = 0
   $ roulettesafia = 0
   "Si tu fais au-dessus de 15 tu gagnes."
   "Si je fais au dessus de 10 je gagne."
   $ roulettejoueur += renpy.random.randint(1,20)
   "Vous avez fait [roulettejoueur]."
   $ roulettesafia += renpy.random.randint(1,20)
   "Safia a fait [roulettesafia]."
   jump jeux1

