    $ indice3 = 0
    $ indice4 = 0
    $ indice5 = 0
    $ couteau = 0
    $ reconfort = 0
    jump enquete

label enquete:
    if inspection >= 4:
        $ inspection = 3
    if deduction >= 4:
        $ deduction = 3
    if inspection <= 0:
        $ inspection = 0
    if deduction <= 0:
        $ deduction = 0
    "Qui allez vous voir?"
    "Votre niveau d'inspection est de [inspection]."
    "Votre niveau de déduction est de [deduction]."
    call screen enquete

