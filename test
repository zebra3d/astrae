label deliax:
   if vie <= 0:
       jump mort
   if creature >= 120:
       play music shacrow
       "Vous gagnez 70 gold."
       $ gold += 70
       $ creature = 0
       jump melitasa
   play music fight
   scene combatdragon
   "Deliax se dresse devant vous."
   "Le combat s'engage."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,40)
   "Il vous porte un énorme coup."
   "Il vous reste [vie] vie."
   scene black
   "{a=jump:deliax}frapper encore{/a}  "
   jump deliax

