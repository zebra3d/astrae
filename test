       "Remi" "*triste*"
       $ chance = renpy.random.randint(1,10)
       if chance == 10:
           "Vous trouvez des cheveux."
           if Mioru in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 1
               jump jour1
           if Scara in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 2
               jump jour1
           if Jinks in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 3
               jump jour1
           if yui in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 4
               jump jour1
           if Elayna in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 5
               jump jour1
           if soraya in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 6
               jump jour1
           if Seikyo in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 7
               jump jour1
           if Rubie in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 8
               jump jour1
           if Dimitri in range(9,11):
               "On dirait les cheveux de quelqu'un..."
               $ cheveux = 9
               jump jour1
       if chance == 9:
           "Vous trouvez des cheveux de Seikyo"
           $ cheveux = 7
           jump jour1
       if chance == 8:
           "Vous trouvez des cheveux de Yui"
           $ cheveux = 4
           jump jour1
       if chance <= 7:
           "Vous ne trouvez rien d'intéressant."
           jump jour1
   if Dimitri in range(0,6):
       show Dimitri
       "Dimitri" "Personne ne veut de moi dans sa vie…"
       "Dimitri" "Pourquoi personne ne m'aime…"
       "Dimitri" "Personne ne m’a demandé de faire d'alliance avec eux!?"
       "Dimitri" "Je vais me suicider…"
       hide Dimitri
       jump jour1
   if Dimitri == 6:
       show Dimitri
