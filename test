label loupgarou03:
   if joueur in range(9,11):
       jump noxus3
   $ mort = 0
   $ mort = renpy.random.randint(1,10)
   if Mioru in range(9,11):
       if mort == 1:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if Scara in range(9,11):
       if mort == 2:
