label loupgarou02:
   if joueur in range(9,11):
       jump noxus2
   $ mort = 0
   $ mort = renpy.random.randint(1,10)
   if Mioru in range(9,11):
       if mort == 1:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if Scara in range(9,11):
       if mort == 2:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if Jinks in range(9,11):
       if mort == 3:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if yui in range(9,11):
       if mort == 4:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if Elayna in range(9,11):
       if mort == 5:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if soraya in range(9,11):
       if mort == 6:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if Seikyo in range(9,11):
       if mort == 7:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if Rubie in range(9,11):
       if mort == 8:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if Dimitri in range(9,11):
       if mort == 9:
           $ mort = 0
           $ mort = renpy.random.randint(1,10)
   if mort == 1:
       if personne1 == 0:
