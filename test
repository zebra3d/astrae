label Bunda4:
   show Bunda
   "Bunda" "Malheureusement, tu n’as pas les traits que je recherche… "
   "Bunda" "Donc tu n'auras pas d'avantage!"
   "Bunda" "Ca va rajouter du piment au jeu!"
   "Bunda" "Alors, on y va!"
   hide Bunda
   jump arene


label armement:
   "Bunda" "Voici 4 objets que tu peux choisir."
   "{a=jump:grandepotionpv}GrandePotionDePV{/a} ; {a=jump:grandepotionm}GrandePotionDeMental{/a} ; {a=jump:armem}Armes{/a} ; {a=jump:rune}Rune{/a}"
   jump armement

label grandepotionpv:
   "Vous prenez la grande potion de pv."
   "Vous gagnez +50 pv."
   $ vie += 50
   jump arene

label grandepotionm:
   "Vous prenez la grande potion mental."
   "Vous gagnez +50 mental."
   $ mental += 50
   jump arene

label armem:
   "Vous prenez l'arme, elle vous donne +15 attaque."
   $ attaque += 15
   $ armement += 15
   $ arme += 1
   jump arene

label rune:
   "Vous prenez la rune, elle vous donne +10 attaque."
   $ attaque += 10
   jump arene

label arene:
   if arene == 1:
       "Bunda" "Vous avez réussi à terrasser le pRemier monstre !"
       "Bunda" "Êtes vous prêt pour le prochain?"
       "{a=jump:combatarene2}Oui{/a} ; {a=jump:desert1}Non{/a} ; {a=jump:objet}Acheté un objet{/a} "
       jump combatarene2
   if arene == 2:
       "Bunda" "Vous avez réussi à terrasser le deuxième monstre !"
       "Bunda" "Êtes vous prêt pour le prochain?"
       "{a=jump:combatarene3}Oui{/a} ; {a=jump:desert1}Non{/a} ; {a=jump:objet}Acheté un objet{/a} "
       jump combatarene3
   if arene == 3:
       "Bunda" "Vous avez réussi à terrasser le troisième monstre !"
       "Bunda" "Êtes vous prêt pour le prochain?"
       "{a=jump:combatarene4}Oui{/a} ; {a=jump:desert1}Non{/a} ; {a=jump:objet}Acheté un objet{/a} "
       jump combatarene4
   if arene == 4:
       "Bunda" "Vous avez réussi à terrasser le quatrième monstre !"
       "Bunda" "Êtes vous prêt pour le prochain?"
       "{a=jump:combatarene5}Oui{/a} ; {a=jump:desert1}Non{/a} ; {a=jump:objet}Acheté un objet{/a} "
       jump combatarene5
   if arene == 5:
       "Bunda" "Vous avez réussi à terrasser le cinquième monstre !"
       "Bunda" "Êtes vous prêt pour le prochain?"
       "{a=jump:combatarene6}Oui{/a} ; {a=jump:desert1}Non{/a} ; {a=jump:objet}Acheté un objet{/a} "
       jump combatarene6
   if arene == 6:
       "Bunda" "Vous avez réussi à terrasser le dernier monstre !"
       "Bunda" "Vous allez devoir vaincre le champion de l'arène."
       "Bunda" "Il se trouve que je suis le champion, alors es tu prêt?"
       "{a=jump:combatarene7}Oui{/a} ; {a=jump:desert1}Non{/a} ; {a=jump:objet}Acheté un objet{/a} "
       jump combatarene7

   scene decorsarene
   "Vous entrez dans l'arène."
   "Voulez vous combattre?"
   "{a=jump:combatarene}Oui{/a} ; {a=jump:desert1}Non{/a}"
   jump combatarene

label combatarene:
   if vie <= 0:
       jump mort
   if creature >= 60:
       play music desert
       "Vous gagnez 10 gold."
       $ arene += 1
       $ gold += 10
       $ creature = 0
       scene decorsarene
       jump arene
   play music fight
   scene combatScarabe
   "La créature se dresse devant vous."
   "Le combat s'engage."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,15)
   "Elle vous porte un énorme coup."
   "Il vous reste [vie] vie."
   scene decorsarene
   "{a=jump:combatarene}frapper encore{/a}  "
   jump combatarene

label combatarene2:
   if vie <= 0:
       jump mort
   if creature >= 40:
       play music desert
       "Vous gagnez 20 gold."
       $ arene += 1
       $ gold += 20
       $ creature = 0
       hide combatraptor
       jump arene
   play music fight
   show combatraptor
   with vpunch
   "La créature se dresse devant vous."
   "Le combat s'engage."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,20)
   "Elle vous porte un énorme coup."
   "Il vous reste [vie] vie."
   hide combatraptor
   "{a=jump:combatarene2}frapper encore{/a}  "
   jump combatarene2

