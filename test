label pirate1:
   if mytho == 1:
       "Vous essayez de mentir au capitaine !"
       "Mais il a tout de suite comprit que vous aviez une langue de serpent."
       $ mental -= 10
       if mental <= 0:
           jump crise
       jump intervenir1
   if immature == 1:
       "Vous êtes trop immature pour faire quelque chose finalement…"
       "Le pirate vous regarde avec mépris."
       $ mental -= 15
       if mental <= 0:
           jump crise
       jump intervenir1
   if sanguinaire == 1:
       "Vous montrez au capitaine que vous n'avez pas froid aux yeux. Vous prenez son sabre et vous vous tranchez le bras."
       "Le capitaine vous regarde avec surprise et trouve que vous êtes courageux."
       "Au lieu de perdre du mental, vous en gagnez !"
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       $ vie -= 10
       if vie <= 0:
           jump mort
       jump intervenir
   if instable == 1:
       $ personalite = 0
       $ personalite += renpy.random.randint(1,5)
       if personalite == 1:
           "Louis Ou Tom" "Je peux vous proposer mes services, je vois que vous avez plein de cicatrices partout… laissez-moi vous soigner!"
           "Le capitaine accepte."
           "Au lieu de perdre du mental, vous en gagnez !"
           $ mental += 10
           if mental >= 100:
               $ mental = 100
           jump capitaine
       if personalite == 2:
           "KevinLeThug" "Uesh gros azy fait pas le malin."
           "Le capitaine vous regarde bizarrement…"
           "Capitaine" "C'est quoi ce langage inconnu? ça m'a l'air agressif… ça me plait!"
           "KevinLeThug" "C'est le langage de la cité mon poto."
           "Le capitaine apprécie ce langage."
           "Au lieu de perdre du mental, vous en gagnez !"
           $ mental += 10
           if mental >= 100:
               $ mental = 100
           jump capitaine
       if personalite == 3:
           "Alexa" "Bonjour, je suis Alexa, votre appareil connecté au réseau universelle."
           "Alexa" "Je vais vous intituler mon titre “chanson de pirate”."
           "Alexa" "Ohé Ohé on est des pirates, on pue on se gratte lalala…"
           "Alexa" "Mille sabor ou est passé mon or."
           "Vous continuez de chanter, le pirate commence à danser sous vos paroles…"
           "Vous gagnez le double de point de mental!"
           $ mental += 10
           if mental >= 100:
               $ mental = 100
           jump capitaine
       if personnalite == 4:
           "Vous prenez l'apparence de Timmy."
           "Il a tellement peur qu'il fait pipi sur lui."
           "Le capitaine s'esclaffe de rire."
           "Pirate" "Toi, gamin, tu m’as bien fait rire! Par conséquent, je veux bien te proposer un deal."
           "Au lieu de perdre du mental, vous en gagnez !"
           $ mental += 10
           if mental >= 100:
               $ mental = 100
           jump capitaine
       if personalite == 5:
           "Faux Narrateur" "Je suis le grand narrateur !"
           "A entendre votre façon de parler, le pirate vous prend pour un genre de dieu…"
           "Gloire au grand narrateur!"
           "Au lieu de perdre du mental, vous en gagnez!"
           $ mental += 10
           if mental >= 100:
               $ mental = 100
           jump capitaine
   if fusil == 1:
       "Vous proposez au capitaine de prendre ce revolver, en lui disant que c'est une arme surpuissante."
       jump capitaine
   if persuasif == 1:
       "Vous êtes persuasif, le capitaine croit en vos paroles."
       $ mental += 5
       if mental >= 100:
           $ mental = 100
       jump capitaine
   if econome == 1:
       "Vous êtes économe, vous savez marchander avec le capitaine."
       $ mental += 2
       if mental >= 100:
           $ mental = 100
       $ gold += 10
       jump capitaine
   if charmeur == 1:
       "Vous êtes quelqu'un de charmant, le capitaine vous écoute attentivement."
       $ mental += 5
       if mental >= 100:
           $ mental = 100
       jump capitaine
   "Vous essayez de convaincre le capitaine, mais son regard méprisant sur vous vous fait perdre vos moyens…"
   "Vous perdez -20 mental."
   $ mental -= 20
   if mental <= 0:
       jump crise
   jump intervenir1

