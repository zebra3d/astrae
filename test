label fincroisiere1:
   "Qui est le coupable?"
   menu confiance:

       "Enzo.":
           $ Enzo1 = 1
           jump fincroisiere2

       "Safia.":
           $ safia1 = 1
           jump fincroisiere2

       "Armand.":
           $ Armand1 = 1
           jump fincroisiere2

       "Marie":
           $ Marie1 = 1
           jump fincroisiere2

       "Yae":
           $ Yae1 = 1
           jump fincroisiere2

       "Gregoire":
           $ Gregoire1 = 1
           jump fincroisiere2

label fincroisiere2:
   if Enzo1 == 1:
       "Ce n'est pas lui."
       jump fincroisiere
   if safia1 == 1:
       "Ce n'est pas elle."
       jump fincroisiere
   if Armand1 == 1:
       "Ce n'est pas lui."
       jump fincroisiere
   if Marie1 == 1:
       "Ce n'est pas elle."
       jump fincroisiere
   if Gregoire == 1:
       "Ce n'est pas lui."
       jump fincroisiere
   if Yae1 == 1:
       show Yae
       "Yae" "Oui, bon d'accord… j'avoue que c'est moi! je n'ai pas voulu donner l'émeraude parce que c'est le seul souvenir de ma mère!"
       "Yae" "Tenez, la voilà votre foutue émeraude!"
       hide Yae
       jump croisierefin

label fincroisiere:
   scene fincroisiereenfer
   play music fincroisiere
   if emeraude == 1:
       $ inventaire.remove(emeraude2)
   if sentimental == 1:
       $ inventaire.remove(coeur2)
   if sextant == 1:
       $ inventaire.remove(sextant2)
   if carte == 1:
       $ inventaire.remove(carte2)
   "Vous n'avez pas trouvé le coupable."
   "De puissants éclairs jaillissent du ciel."
   "Les vagues secouent le bateau dans tous les sens."
   "Vous êtes piégé dans le temps."
   "Avec tout le monde à jamais ici!"
   "Vous mourirez ici!"
   "Qui a bien pu nous cacher quelque chose?"
   "Vous allez mourir ici… sous cette tempête destructrice!"
   "Jusqu'à ce que le navire sombre dans l'océan…"
   "Et qu'il vous emporte jusqu'à la mort…"
   "Vous avez récupéré un morceau de souvenir."
   "*Croisiere d'enfer*"
   return

label croisierefin:
   "Une dernière tempête fait rage sur le bateau."
   "Si vous avez une corde, vous pouvez peut-être réussir à stabiliser le bateau."
