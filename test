   hide barde
   "Le barde a l'air de vouloir que vous écoutiez c'est chanson"
   $ bulle = 0
   $ chanson = 0
   $ carton = 0
   $ magnifique = 0
   $ chevalier = 0
   $ narrateur = 0
   $ aventurier = 0

   menu troubadour:

       "Bulle de Lait":
           if bulle == 1:
