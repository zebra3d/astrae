label noxus4:
   if voyante == 1:
       $ voyance = 0
       $ voyance = renpy.random.randint(1,10)
       if voyance in range(9,11):
           "La voyante a vu un loup."
       if voyance in range(0,9):
           "La voyante a vu un innocent."

