       "Votre lumiere reste présente"
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump lumios1
   if herboriste == 1:
       "Votre lumiere reste présente"
       $ mental += 10
       if mental >= 100:
           $ mental = 100
       jump lumios1
   "Votre lumiere ce faiblit"
   $ mental -= 40
   if mental <= 2:
       jump crise
   jump lumios1

