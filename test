label aqua:
   show fred
   "Bon, très bien sacamouche! t’as réussi, je suis fier de toi!"
   "Maintenant, va me chercher de la poiscail!"
   hide fred
   $ poisson = 0
   $ medaka = 0
   $ pieuvre = 0
   $ tortue = 0
   $ fish = 0
   $ meduse = 0
   $ blob = 0
   $ tentacule = 0
   $ pirana = 0
   jump peche

label peche:
   if tentacule == 3:
       jump fintentacule
   "Voulez vous pêcher?"
   $ peche = 0
   $ peche += renpy.random.randint(1,40)
   "{a=jump:peche1}Oui{/a} ;{a=jump:peche2}Non{/a} "
   jump peche

label peche1:
   if peche in range(1,6):
       $ peche = 0
       show pirana
       "Vous avez pêché un piranha."
       "Il vous a mordu…"
       "Vous perdez -2 pv."
       $ vie -= 2
       hide pirana
       $ pirana += 1
       if vie <= 0:
           jump mort
       jump peche
   if peche in range(5,8):
       $ peche = 0
       show medaka
       "Vous avez pêché un poisson éclatant."
       "Il vous donne 2 pv."
       $ vie += 2
       if vie >= 100:
           $ vie = 100
       hide medaka
       $ medaka += 1
       jump peche
   if peche in range(9,16):
       $ peche = 0
       $ pieuvre += 1
       show pieuvre
       "Vous avez pêché une pieuvre."
       hide pieuvre
       jump peche
   if peche in range(15,20):
       $ tortue += 1
       show tortue
       "Vous avez pêché une tortue."
       hide tortue
       $ peche = 0
       jump peche
   if peche == 20:
       $ peche = 0
       show tentacule
       "Vous avez pêché un saraka."
       hide tentacule
       jump saraka
   if peche in range(20,30):
       $ peche = 0
       $ blob += 1
       show blobfish
       "Vous avez pécher un blob fish."
       "Vous perdez -5 mental."
       hide blobfish
       $ mental -= 5
       if mental <= 0:
           jump crise
       jump peche
   if peche in range(30,35):
       $ peche = 0
       $ fish += 1
       show poisson
       "Vous avez pêché un poisson dragon."
       hide poisson
       jump peche
   if peche in range(35,40):
       $ peche = 0
       $ meduse += 1
       show meduse
       "Vous avez péché une méduse."
       "Elle vous rend +2 mental."
       $ mental += 2
       if mental >= 100:
           $ mental = 100
       hide meduse
       jump peche

label saraka:
   if vie <= 0:
       jump mort
   if creature >= 50:
       play music plage
       "Vous gagnez 5 gold."
       scene decorsplage
       $ gold += 5
       $ tentacule += 1
       $ creature = 0
       jump peche
   play music fight2
   show tentacule
   with vpunch
   "Le saraka se dresse devant vous."
   "Le combat s'engage."
   $ creature += renpy.random.randint(1,20)
   $ creature += attaque
   "Vous portez un coup d'attaque de [creature]."
   $ vie -= renpy.random.randint(1,5)
   "Il vous porte un énorme coup."
   "Il vous reste [vie] vie."
   "{a=jump:saraka}frapper encore{/a};  "
   jump saraka

