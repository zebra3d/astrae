label start:
    show screen sacequip
    "Voici votre équipement je vous ferais un retour des que possible"
    hide screen sacequip
    show equipement
    "Voici votre fiche personnage"
    hide equipement
    "Vous pouvez l'ouvrir a tout moment"
    show danger
    "Ce bouton vous ramene a un jeu simple sans insulte tout en courtoisi"
    "Si vous n'aimer pas le style du narrateur qui embete le joueur cliquer sur ce bouton"
    hide danger
    $ passivguerrier = 0
    $ charge = 0
    $ tank = 0
    $ poison = 0
    $ tranchelame = 0
    $ daguedestenebre = 0
    $ soin = 0
    $ cristalisation = 0
    $ magiedeselement = 0
    $ flechedefeu = 0
    $ precision = 0
    $ sniper = 0
    $ equipement = 0
    $ torse = 0
    $ casque = 0
    $ jambe = 0
    $ bras = 0
    $ epee = 0
    $ arc = 0
    $ magie = 0
    $ assa = 0
    $ passe = 0
    $ pied = 0
    jump startx

