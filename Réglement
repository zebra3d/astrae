label starttrue:
    $ vigoureux = 0
    $ passe = 0
    $ esquive = 0
    $ poison = 0
    $ acte1x = 0
    $ acte2x = 0
    $ acte3x = 0
    $ charisme = 0
    $ force = 0
    $ talent = 0
    $ intelligence = 0
    $ narafutur = 0
    $ narapasse = 0
    $ aleatoire = 0
    menu naralum:
        "Narrateur de Lumiere":
            "En choisisant le narrateur de lumiere vous n'aurez pas tout les succes du début"
            "Le Narrateur du futur vous donne les compétance et les talent aléatoirement"
            "Pas besoin de se Casser la Tête"
            $ narafutur = 1
            $ aleatoire = 1
            "Sinon vous pouvez décider d'étre le patient0"
            "Les patient0 a besoin juste d'appuyer sur continuer"
            "parcontre les patient0 non pas de talent"
            "Je déconne bien sur tête à cheveux"
            jump carismex


        "Narrateur Roleplay":
            "En choisisant le narrateur roleplay tu décide a renoncé au succés"
            $ passe = 1
            jump roleplay1

        "Narrateur Normal":
            jump grounds
label carismex:
    "{a=jump:carisme2}Charisme{/a};{a=jump:intel2}Intelligence{/a};{a=jump:force2}Force{/a};"
    jump carismex

label carisme2:
    show screen charisme
    $ charisme = 100
    jump allan

label force2:
    show screen force
    $ force = 100
label intel2:
    show screen intelligence
    $ intelligence = 100
    jump allan

label roleplay1:
    if passe == 1:
        jump grounds
    $ passe = 0
    $ mana = 0
    $ basic = 0
    $ cle = 0
    $ clee100000 = 1
    if basic = 1:
        if basic = 0:
            "Vous n'avez pas accés a ce programme"
        elif = 1:
            jump cle
        if clee100000 = 0:
            "Vous n'avez pas la cle pour avoir votre chemin"

label cle:
    $ clee100000 = 1
    $ clee100000 = renpy.random.randint(0,1000000)
    if cleclee100000 >= 0:
        jump reda

